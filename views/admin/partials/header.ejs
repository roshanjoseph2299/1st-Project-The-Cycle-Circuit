     <!-- style for table in category -->
    <style>
        /* Add responsive styles to the table */
        .table-responsive {
            
          overflow-x: auto;
        }
        
        table {
          width: 50%;
         
        }
        
        th, td {
          padding: 8px;
          text-align: left;
        
        }
        
        /* Adjust the table for smaller screens */
        @media screen and (max-width: 600px) {
          table {
            display: block;
            overflow-x: auto;
          }
          
          th, td {
            display: block;
            width: 100%;
          }
        }
      </style>

      <!-----------------for styling pagination----------------->
      <style>.pagination-container {
        
